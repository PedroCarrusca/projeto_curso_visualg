algoritmo "variaveis_e_constantes"

var //Declaração de variáveis
   numero1, numero2: inteiro
   media: real
   nome: caractere
   logado: logico
inicio //Colocando valores as variáveis
      numero1 <- 2
      numero2 <- -25
      media <- 2.5
      nome <- "Pedro Carrusca"
      logado <- verdadeiro
      
      leia(nome)
      escreva(nome)
fimalgoritmo
