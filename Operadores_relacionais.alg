algoritmo "Operadores Relacionais"
// Função :
// Autor :
// Data : 21/07/2022
// Seção de Declarações 
var

   n1, n2: real
   
inicio
// Seção de Comandos

   n1:= 10
   n2:= 15
   
   se (n1 = n2) entao
     escreval("Nota 1 é diferente")
   senao
     escreval("Nota 1 é igual")
   fimse
   
   
fimalgoritmo
